<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="jobclient" nsURI="http://www.misc.com/common/moplaf/job/client/model/0.1"
    nsPrefix="jbc">
  <eClassifiers xsi:type="ecore:EClass" name="JobRemote">
    <eOperations name="onProgress">
      <eParameters name="feedback" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//ProgressFeedback"/>
    </eOperations>
    <eOperations name="onReturn">
      <eParameters name="feedback" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//ReturnFeedback"/>
    </eOperations>
    <eOperations name="refreshProgress"/>
    <eOperations name="run" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//ReturnFeedback">
      <eParameters name="job" eType="#//SubmittedJob"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="SubmissionID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Result" eType="#//JobRemoteResult"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="LastProgress" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="LastProgressWork" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EFloat"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="LastProgressTask" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="job" upperBound="-1" eType="ecore:EClass ../../com.misc.common.moplaf.job.emf/model/job.ecore#//Job"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="jobremote" eType="#//JobRemote"
        eOpposite="#//JobRemote/jobremoteeOpposite"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="jobremoteeOpposite" eType="#//JobRemote"
        eOpposite="#//JobRemote/jobremote"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Progress" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//ProgressFeedback"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="StartTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JobEngineProxy" abstract="true">
    <eOperations name="submitJob" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt">
      <eParameters name="job" eType="#//JobRemote"/>
    </eOperations>
    <eOperations name="getJob" eType="#//JobRemote">
      <eParameters name="submissionID" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JobRemoteResult"/>
  <eClassifiers xsi:type="ecore:EClass" name="JobEngine" abstract="true">
    <eOperations name="start"/>
    <eOperations name="stop"/>
    <eOperations name="onJobProgress" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean">
      <eParameters name="job" eType="#//SubmittedJob"/>
      <eParameters name="progress" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//ProgressFeedback"/>
    </eOperations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="SubmittedJobs" upperBound="-1"
        eType="#//SubmittedJob" containment="true" eOpposite="#//SubmittedJob/JobEngine"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Running" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        transient="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="StartFeedback" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//EnabledFeedback"
        changeable="false" volatile="true" transient="true" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="StopFeedback" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//EnabledFeedback"
        changeable="false" volatile="true" transient="true" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Label" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        changeable="false" volatile="true" transient="true" derived="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SubmittedJob">
    <eStructuralFeatures xsi:type="ecore:EReference" name="Job" eType="#//JobRemote"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="JobEngine" lowerBound="1"
        eType="#//JobEngine" eOpposite="#//JobEngine/SubmittedJobs"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JobEngineInProcess" eSuperTypes="#//JobEngineProxy">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Asynchronous" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JobScheduler">
    <eOperations name="start"/>
    <eOperations name="stop"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="Jobs" upperBound="-1" eType="#//JobRemote"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="jobschedueled" upperBound="-1"
        eType="#//JobScheduled" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="engine" upperBound="-1"
        eType="#//Engine" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="StartFeedback" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//EnabledFeedback"
        changeable="false" volatile="true" transient="true" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="StopFeedback" eType="ecore:EDataType ../../com.misc.common.moplaf.job.emf/model/job.ecore#//EnabledFeedback"
        changeable="false" volatile="true" transient="true" derived="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Started" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"
        defaultValueLiteral="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nbOfEngines" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nbOfJobs" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="JobScheduled">
    <eOperations name="kill"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="job" lowerBound="1" eType="ecore:EClass ../../com.misc.common.moplaf.job.emf/model/job.ecore#//Job"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ScheduledOn" eType="#//Engine"
        eOpposite="#//Engine/jobschedueled"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Engine">
    <eStructuralFeatures xsi:type="ecore:EReference" name="jobschedueled" eType="#//JobScheduled"
        eOpposite="#//JobScheduled/ScheduledOn"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="Name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
