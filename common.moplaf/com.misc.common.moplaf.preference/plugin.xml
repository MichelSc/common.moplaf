<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.misc.common.moplaf.preference.WorkbenchPreferencePageMain"
            id="com.misc.common.moplaf.preference.pageMain"
            name="Moplaf">
      </page>
      <page
            category="com.misc.common.moplaf.preference.pageMain"
            class="com.misc.common.moplaf.preference.WorkbenchPreferencePageLogic"
            id="com.misc.common.moplaf.preference.pageLogic"
            name="Logic">
      </page>
      <page
            category="com.misc.common.moplaf.preference.pageMain"
            class="com.misc.common.moplaf.preference.WorkbenchPreferencePageViews"
            id="com.misc.common.moplaf.preference.pageViews"
            name="Views">
      </page>
      <page
            category="com.misc.common.moplaf.preference.pageMain"
            class="com.misc.common.moplaf.preference.PreferencePage"
            id="com.misc.common.moplaf.preference.page"
            name="Application">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.misc.common.moplaf.preference.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="com.misc.common.moplaf.preference.Startup">
      </startup>
   </extension>

</plugin>
